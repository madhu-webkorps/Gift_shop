<div class="field"><div class="container1">
    <div class="row">
        <div class="col-md-6">
            <h1 class = "w3-center">Sign up</h1>
            <%= form_for(resource, as: resource_name , class:"w3-container",url: registration_path(resource_name)) do |f| %>
            <%= render "devise/shared/error_messages", resource: resource %>

 

            <div class="field">
            <label>Email</label>
            <%= f.email_field :email, autofocus: true, autocomplete: "email",class:"w3-input" , placeholder: "Enter your Email Id" %>
            </div>

            <div class="field">
            <label>First Name</label>
            <%= f.text_field :frist_name, autofocus: true, autocomplete: "email",class:"w3-input" , placeholder: "Enter your Frist Name" %>
            </div>

            <div class="field">
            <label>Last Name</label>
            <%= f.text_field :last_name, autofocus: true, autocomplete: "email",class:"w3-input" , placeholder: "Enter your Last Name" %>
            </div>

            <div class="field">
            <label>Address</label>
            <%= f.text_field :address, autofocus: true, autocomplete: "email",class:"w3-input" , placeholder: "Enter your Address" %>
            </div>

            <div class="field">
            <label>Pin Code</label>
            <%= f.text_field :pin_code, autofocus: true, autocomplete: "email",class:"w3-input" , placeholder: "Enter your Pin Code" %>
            </div>

            <div class="field">
            <label>Phone_no</label>
            <%= f.text_field :phone_no, autofocus: true, autocomplete: "email",class:"w3-input" , placeholder: "Enter your Phone number" %>
            </div>



            <div class="field">
            <%= f.label :password %>
            <% if @minimum_password_length %>
            <em>(<%= @minimum_password_length %> characters minimum)</em>
            <% end %><br />
            <%= f.password_field :password, autocomplete: "new-password", placeholder: "Enter your Password atleast 6 digits"  %>
            </div>

            <div class="field">
            <%= f.label :password_confirmation %><br />
            <%= f.password_field :password_confirmation, autocomplete: "new-password" , placeholder: "Enter confrim password" %>
            </div><br>

            <%= f.label :image %>
            <%= f.file_field :image%>
            <br>

            <div class="actions">
            <%= f.submit "Sign up" ,class: "btn btn-primary float-right"%>
            </div>
            <% end %>
<br><br>
            <%= render "devise/shared/links" %>
            </div>
        </div>
    </div>
