<div class="">
   <%= form_with(model: @category , local: false) do |form| %>
      <div id="errorArea">
        
      </div>
         
      <div class="form-group mt-5 w-25 ">
        <%= form.label :name, class: "form-label" %>
        <%= form.text_field :name, class:"form-control"%>
      </div>
      
      <div class="form-group mt-5 w-25 ">
        <%= form.label :status, class: "form-label" %>
        <%= form.text_field :status, class:"form-control"%>
      </div>
      
      



      
      <div class="actions">
        <%= form.submit  class:"btn btn-success  ml-5"%>
      </div>
      
</div>
  

  <% end %>



